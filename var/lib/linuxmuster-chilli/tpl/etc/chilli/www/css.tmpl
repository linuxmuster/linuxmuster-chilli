p {
  color: #333;
}
table {
   margin: auto;
}
td {
   color: #333;
   padding: 3px;
}
li {
   color: #333;
   padding: 3px 0 3px 0;
}
h1 {
    margin: 0;
    padding: 0;
    font-family: Verdana, sans-serif;
    font-size: 20px;
    color: #999999;
    font-weight: bold;
    text-shadow: 2px 2px 0 #FFFFFF;
}
#login, #contact, #register, #map {
   padding-bottom: 15px;
   margin: 0 1em;
}
#map {
   width: 100%;
}
#login_form, #contact_form, #register_form {
   text-align: center;
   margin: auto;
}
#header {
   margin: 0 1em;
   height: 80px;
   line-height: 80px;
   vertical-align: center;
   text-align: middle;
}
#body {
   clear: both;
   margin: 0 1em;
   padding: 10px 10px 2em 10px !important;
   border: 1px solid #999 !important;
   background-color: #fff;
}
#footer { 
   color: #666;
   font-size: 80%;
   clear: both;
   padding-top: 20px;
   margin: auto;
}
#terms {
   text-align: left;
   margin: auto;
}
body {
   margin: 0;
   padding: 0;
   text-align: center;
   font-family: arial, helvetica, sans-serif;
   background-color: #FBFAF9;
   color: #333;
}
#openid_url {
  background: url(openid.gif) no-repeat;
  background-color: #fff;
  background-position: 0 50%;
  color: #000;
  padding-left: 18px;
}
#openid-label,#login-label {
  font-size: smaller;
  line-height: 14px;
  vertical-align: middle;
}
.err{
   font-weight: bold;
   color: #000;
   padding:1em;
   margin: 0 0 1em 0;
   boder: 1px solid #ccc;
   background-color: #dd6400;
}

.success {
   font-weight: bold;
   color: #000;
   padding:1em;
   margin: 0 0 1em 0;
   boder: 1px solid #ccc;
   background-color: #A9D400;
}

div.powered {
    margin-top: 12px;
    font-size: 80%;
}


a {
    color: #11386B;
    font-weight: bold;
    text-decoration: underline;
}

a:hover {
    color: #6695D2;
}

